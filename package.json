{
  "name": "mini-i-ui",
  "version": "0.1.0",
  "type": "module",
  "description": "",
  "main": "./dist/i-ui.umd.js",
  "module": "./dist/i-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/i-ui.es.js",
      "require": "./dist/i-ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "homepage": "www.i-ui.top",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs:dev": "vitepress dev docs --config ./vite.config.ts",
    "docs:build": "vitepress build docs --config ./vite.config.ts",
    "docs:preview": "vitepress preview docs --config ./vite.config.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint --fix --ext .ts,.tsx,.vue src docs vite.config.ts uno.config.ts --ignore-pattern 'docs/.vitepress/dist/**'",
    "format": "prettier --write \"src/**/*{.ts,.tsx,.vue}\"",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@iconify-json/fluent": "^1.2.19",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/line-md": "^1.2.8",
    "@iconify-json/tabler": "^1.2.17",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.25.1",
    "eslint-formatter-pretty": "^6.0.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "terser": "^5.39.0",
    "vite": "5.4.18",
    "vitepress": "^1.6.3",
    "vitepress-theme-demoblock": "^3.1.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "unocss": "66.1.0-beta.11",
    "vue": "^3.5.13"
  }
}
